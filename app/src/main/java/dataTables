-----------------------------------------
CREATE TABLE captures
    (
      CaptureID TEXT,
      Turtle TEXT,
      CapDate int,
      CatchNum integer primary key,
      Location TEXT,
      StraightCarapaceLengthMinimum real,
      StraightCarapaceLengthNotchTip real,
      StraightCarapaceWidth real,
      HeadWidth real,
      HeadLength real,
      HeadDepth real,
      CurvedCarapaceLengthMin real,
      CurvedCarapaceLengthNotchTip real,
      CurvedCarapaceWidth real,
      Weight real,
      BodyDepth real
    );

